                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:background="@color/cardview_light_background"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/tableborder"
                android:layout_weight="2"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@color/cardview_light_background"
                    android:paddingLeft="20dp"
                    android:gravity="center_vertical"
                    android:layout_margin="1dp"
                    android:layout_weight="1"
                    android:text="Infertile Couple's Information" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:id="@+id/textView13"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:background="@color/cardview_light_background"
                            android:text="Counselled" />

                        <TextView
                            android:id="@+id/infertile_couple_counselled_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                        <TextView
                            android:id="@+id/infertile_couple_counselled_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">
                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/cardview_light_background"
                        android:layout_margin="1dp"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="Referred" />
                        <TextView
                            android:id="@+id/infertile_couple_referred_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                        <TextView
                            android:id="@+id/infertile_couple_referred_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:background="@color/tableborder"
            android:orientation="vertical">

            <dashboard.opensrp.org.jandjdashboard.widgets.VerticalTextView
                android:id="@+id/pnc_label"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginBottom="1dp"
                android:layout_marginTop="1dp"
                android:background="@color/cardview_light_background"
                android:gravity="center"
                android:text="PNC Information" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="1dp"
            android:background="@color/tableborder"
            android:orientation="vertical">

            <dashboard.opensrp.org.jandjdashboard.widgets.VerticalTextView
                android:id="@+id/mother_label"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@color/cardview_light_background"
                android:gravity="center"
                android:text="Mother" />

            <dashboard.opensrp.org.jandjdashboard.widgets.VerticalTextView
                android:id="@+id/baby_label"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginBottom="1dp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_weight="1"
                android:background="@color/cardview_light_background"
                android:gravity="center"
                android:text="Baby" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/tableborder"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pnc_current_visit_one_label"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="2"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="Visit-1(in 24 Hours)" />

                <TextView
                    android:id="@+id/pnc_current_visit_one"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:background="@color/cardview_light_background"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:text="" />

                <TextView
                    android:id="@+id/pnc_service_visit_one"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:background="@color/cardview_light_background"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/tableborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pnc_current_visit_two_label"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="2"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Visit-2(in 2-3 Days)" />

                <TextView
                    android:id="@+id/pnc_current_visit_two"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:text="" />

                <TextView
                    android:id="@+id/pnc_service_visit_two"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:text="" />
            </LinearLayout>


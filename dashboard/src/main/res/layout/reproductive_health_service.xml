<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="2dp"
        android:elevation="3dp"
        android:padding="20dp">

        <TextView
            android:id="@+id/filtertitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="Month: this Week"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/iconcolorsused" />

    </android.support.v7.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="@color/tableborder"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="1dp"
            android:layout_weight="7"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/blankplcaeholder1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/infolabel"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Info" />

            <TextView
                android:id="@+id/servicelabel"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Service" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="20dp"
            android:layout_height="match_parent"
            android:orientation="horizontal"></LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/blankplcaeholder2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/currentlabel"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Current" />

            <TextView
                android:id="@+id/servicelabel2"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Service" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="5dp"
        android:background="@color/cardview_light_background"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:weightSum="13"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:background="@color/tableborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/anc_info_label"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@color/cardview_light_background"
                    android:layout_margin="1dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingLeft="20dp"
                    android:text="ANC Information" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/anc_visit_one_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:layout_weight="2"
                            android:background="@color/cardview_light_background"
                            android:gravity="center"
                            android:text="Visit-1(in 4 months)" />

                        <TextView
                            android:id="@+id/anc_visit_1_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                        <TextView
                            android:id="@+id/anc_visit_1_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/anc_visit_two_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:layout_weight="2"
                            android:background="@color/alttablerow"
                            android:gravity="center"
                            android:text="Visit-2(in 6 months)" />

                        <TextView
                            android:id="@+id/anc_visit_2_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />

                        <TextView
                            android:id="@+id/anc_visit_2_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/anc_visit_three_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:layout_weight="2"
                            android:background="@color/cardview_light_background"
                            android:gravity="center"
                            android:text="Visit-3(in 8 months)" />

                        <TextView
                            android:id="@+id/anc_visit_3_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                        <TextView
                            android:id="@+id/anc_visit_3_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/anc_visit_four_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:layout_weight="2"
                            android:background="@color/alttablerow"
                            android:gravity="center"
                            android:text="Visit-4(in 9 months)" />

                        <TextView
                            android:id="@+id/anc_visit_4_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />

                        <TextView
                            android:id="@+id/anc_visit_4_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="1dp"
                            android:layout_marginRight="1dp"
                            android:layout_marginTop="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:background="@color/tableborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/number_of_tt_received_woman_label"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@color/cardview_light_background"
                    android:layout_margin="1dp"
                    android:paddingLeft="20dp"
                    android:gravity="center_vertical"
                    android:layout_weight="1"
                    android:text="Number of Women Received TT" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/first_tt_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="2"
                            android:background="@color/cardview_light_background"
                            android:gravity="center"
                            android:text="1st" />

                        <TextView
                            android:id="@+id/tt_received_1_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                        <TextView
                            android:id="@+id/tt_received_1_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/second_tt_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="2"
                            android:background="@color/alttablerow"
                            android:gravity="center"
                            android:text="2nd" />

                        <TextView
                            android:id="@+id/tt_received_2_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />

                        <TextView
                            android:id="@+id/tt_received_2_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/third_tt_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="2"
                            android:background="@color/cardview_light_background"
                            android:gravity="center"
                            android:text="3rd" />

                        <TextView
                            android:id="@+id/tt_received_3_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                        <TextView
                            android:id="@+id/tt_received_3_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/fourth_tt_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="2"
                            android:background="@color/alttablerow"
                            android:gravity="center"
                            android:text="4th" />

                        <TextView
                            android:id="@+id/tt_received_4_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />

                        <TextView
                            android:id="@+id/tt_received_4_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/alttablerow"
                            android:text="" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/five_tt_label"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="2"
                            android:background="@color/cardview_light_background"
                            android:gravity="center"
                            android:text="5th" />

                        <TextView
                            android:id="@+id/tt_received_5_info"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />

                        <TextView
                            android:id="@+id/tt_received_5_service"
                            android:layout_width="60dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:background="@color/cardview_light_background"
                            android:text="" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/tableborder"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/number_of_ecp_receptors_label"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:layout_weight="1"
                    android:background="@color/alttablerow"
                    android:gravity="center_vertical"
                    android:paddingLeft="20dp"
                    android:text="Number of ECP Receptors" />

                <TextView
                    android:id="@+id/ecp_receptor_info"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:background="@color/alttablerow"
                    android:text="" />

                <TextView
                    android:id="@+id/ecp_receptor_service"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:background="@color/alttablerow"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/tableborder"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/number_of_misoprotol_receptors_label"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:layout_weight="1"
                    android:background="@color/cardview_light_background"
                    android:gravity="center_vertical"
                    android:paddingLeft="20dp"
                    android:text="Number of Misoprostol Tablet Receptors" />
                <TextView
                    android:id="@+id/misoprostol_receptor_info"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:background="@color/cardview_light_background"
                    android:text="" />

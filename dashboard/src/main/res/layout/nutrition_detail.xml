<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboardcategory_detail"
    style="?android:attr/textAppearanceLarge"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/alttablerow"
    android:orientation="vertical"
    android:padding="5dp"
    android:textIsSelectable="true"
    tools:context="dashboard.opensrp.org.jandjdashboard.fragments.dashboardCategoryDetailFragment">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="2dp"
        android:elevation="3dp"
        android:padding="20dp">

        <TextView
            android:id="@+id/filtertitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="Month: this Week"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/iconcolorsused" />

    </android.support.v7.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal">

        <android.support.v7.widget.CardView
            android:id="@+id/elco_card"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:elevation="3dp"
            android:padding="10dp"
            app:cardBackgroundColor="@color/greencardcolor">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                android:scaleType="fitEnd"
                android:src="@drawable/numberlivebirth" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_marginTop="2dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/unit_total_livebirth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|right"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:padding="2dp"
                    android:textAppearance="?android:attr/textAppearanceListItem"
                    android:textColor="@color/textcolor" />

                <TextView
                    android:id="@+id/elco_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|end"
                    android:layout_marginLeft="10dp"
                    android:padding="2dp"
                    android:text="Unit's total number of livebirth"
                    android:textAlignment="textEnd"
                    android:textAppearance="?android:attr/textAppearanceListItemSmall"
                    android:textColor="@color/textcolor"
                    android:textSize="10sp" />
            </LinearLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/new_registered_elco_card"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:elevation="3dp"
            android:padding="10dp"
            app:cardBackgroundColor="@color/pinkcardcolor">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                android:scaleType="fitEnd"
                android:src="@drawable/newelcotranscard" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_marginTop="2dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/total_livebirth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|right"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:padding="2dp"
                    android:textAppearance="?android:attr/textAppearanceListItem"
                    android:textColor="@color/textcolor" />

                <TextView
                    android:id="@+id/newly_registered_elco_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|end"
                    android:layout_marginLeft="10dp"
                    android:padding="2dp"
                    android:text="Total number of livebirth"
                    android:textAlignment="textEnd"
                    android:textAppearance="?android:attr/textAppearanceListItemSmall"
                    android:textColor="@color/textcolor"
                    android:textSize="10sp" />
            </LinearLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/elco_visited_card"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:elevation="3dp"
            android:padding="10dp"
            app:cardBackgroundColor="@color/purplecardcolor">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                android:scaleType="fitEnd"
                android:src="@drawable/numberdeath" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="10dp"
                android:layout_marginTop="2dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/unit_total_death"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|right"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:padding="2dp"
                    android:textAppearance="?android:attr/textAppearanceListItem"
                    android:textColor="@color/textcolor" />

                <TextView
                    android:id="@+id/total_elco_visited_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|end"
                    android:layout_marginLeft="10dp"
                    android:padding="2dp"
                    android:text="Unit's total number of death"
                    android:textAlignment="textEnd"
                    android:textAppearance="?android:attr/textAppearanceListItemSmall"
                    android:textColor="@color/textcolor"
                    android:textSize="10sp" />
            </LinearLayout>

        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/contraceptive_acceptance_card"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_margin="2dp"
            android:layout_weight="1"
            android:elevation="3dp"
            android:padding="10dp"
            app:cardBackgroundColor="@color/lightgreencardcolor">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                android:scaleType="fitEnd"
                android:src="@drawable/deathcard" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="10dp"
                android:layout_marginTop="2dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/total_death"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|right"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:padding="2dp"
                    android:textAppearance="?android:attr/textAppearanceListItem"
                    android:textColor="@color/textcolor" />

                <TextView
                    android:id="@+id/contraceptive_acceptance_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|end"
                    android:layout_marginLeft="10dp"
                    android:padding="2dp"
                    android:text="Total number of death"
                    android:textAlignment="textEnd"
                    android:textAppearance="?android:attr/textAppearanceListItemSmall"
                    android:textColor="@color/textcolor"
                    android:textSize="10sp" />
            </LinearLayout>

        </android.support.v7.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:background="@color/tableborder"
            android:orientation="vertical">

            <TextView
                android:id="@+id/pregnant_and_mother_label"
                android:layout_width="match_parent"
                android:layout_height="22dp"
                android:background="@color/selectedlistred"
                android:gravity="center"
                android:text="Pregnant and Mother 0-23 months"
                android:textColor="@color/cardview_light_background" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/servicetypelabel"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="1"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Service Type" />

                <TextView
                    android:id="@+id/pregnantwomanlabel"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Pregnant Woman" />

                <TextView
                    android:id="@+id/mother_zero_to_twenty_three_months_label"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Mother (0-23 months)" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/iron_and_folic_acid_label"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="1"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="Iron and Folic Acid Tablets and Provided Counselling on Complimentary Food" />

                <TextView
                    android:id="@+id/iron_and_folic_acid_pregnant_woman_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="" />

                <TextView
                    android:id="@+id/iron_and_folic_acid_mother_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/distributed_iron_and_folic_acid_label"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="1"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Distributed Iron and Folic Acid Tablets" />

                <TextView
                    android:id="@+id/distributed_iron_and_folic_acid_pregnant_woman_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="" />

                <TextView
                    android:id="@+id/distributed_iron_and_folic_acid_mother_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/counselling_on_breastfeeding_and_complimentary_food_label"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="1"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="Counselling on breastfeeding and complimentary food" />

                <TextView
                    android:id="@+id/counselling_on_breastfeeding_and_complimentary_food_pregnant_woman_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="" />

                <TextView
                    android:id="@+id/counselling_on_breastfeeding_and_complimentary_food_mother_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/cardview_light_background"
                    android:gravity="center"
                    android:text="" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/counselling_on_feeding_mnp_to_children_label"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="1"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Still Birth" />

                <TextView
                    android:id="@+id/counselling_on_feeding_mnp_to_children_pregnant_woman_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="" />

                <TextView
                    android:id="@+id/counselling_on_feeding_mnp_to_children_mother_info"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_weight="3"
            android:background="@color/tableborder"
            android:orientation="vertical">

            <TextView
                android:id="@+id/children_label"
                android:layout_width="match_parent"
                android:layout_height="22dp"
                android:background="@color/selectedlistred"
                android:gravity="center"
                android:text="Children 0-59 months"
                android:textColor="@color/cardview_light_background" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/servicetypelabel2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="1"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:text="Service Type" />

                <TextView
                    android:id="@+id/zero_to_six_month_label"
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:padding="2dp"
                    android:text="0 - 6 months" />

                <TextView
                    android:id="@+id/six_to_24_month_label"
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:padding="3dp"
                    android:text="6 - 24 months" />

                <TextView
                    android:id="@+id/twentyfour_to_fifty_month_label"
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginTop="1dp"
                    android:background="@color/alttablerow"
                    android:gravity="center"
                    android:padding="3dp"
                    android:text="24 - 50 months" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/feed_colostrum_milk_label"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_weight="1"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:padding="2dp"
                                    android:text="Feed Colostrum Milk within 1 hour after birth" />

                                <TextView
                                    android:id="@+id/feed_colostrum_milk_zero_to_six_month_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:text="" />

                                <TextView
                                    android:id="@+id/feed_colostrum_milk_six_to_24_month_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:text="" />

                                <TextView
                                    android:id="@+id/feed_colostrum_milk_twentyfour_to_fifty_month_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:text="" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/breastfeeding_up_to_6_months_label"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_weight="1"
                                    android:background="@color/alttablerow"
                                    android:gravity="center"
                                    android:padding="2dp"
                                    android:text="Breastfeeding up to 6 months" />

                                <TextView
                                    android:id="@+id/breastfeeding_up_to_6_months_zero_to_six_months_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:background="@color/alttablerow"
                                    android:gravity="center"
                                    android:text="" />

                                <TextView
                                    android:id="@+id/breastfeeding_up_to_6_months_six_to_twentyfour_months_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:background="@color/alttablerow"
                                    android:gravity="center"
                                    android:text="" />

                                <TextView
                                    android:id="@+id/breastfeeding_up_to_6_months_twentyfour_to_fifty_months_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:background="@color/alttablerow"
                                    android:gravity="center"
                                    android:text="" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/providing_extra_complimentary_food_label"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:layout_weight="1"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:padding="2dp"
                                    android:text="Providing extra complimentary food along with the breast milk after 6 months" />

                                <TextView
                                    android:id="@+id/providing_extra_complimentary_food_zero_to_6_months_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:text="" />

                                <TextView
                                    android:id="@+id/providing_extra_complimentary_food_6_to_24_months_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
                                    android:background="@color/cardview_light_background"
                                    android:gravity="center"
                                    android:text="" />

                                <TextView
                                    android:id="@+id/providing_extra_complimentary_food_24_to_50_months_info"
                                    android:layout_width="80dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginRight="1dp"
                                    android:layout_marginTop="1dp"
